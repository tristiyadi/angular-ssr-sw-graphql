    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "mochawesome": "^6.2.2",
    "mochawesome-report-generator": "^5.2.0",
    "protractor": "~7.0.0",
