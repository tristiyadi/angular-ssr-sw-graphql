    "@angular/material": "^11.2.12",
    "@angular/material-moment-adapter": "^11.2.12",
    "@briebug/cypress-schematic": "^5.0.0",
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/typescript": "1.19.0",
