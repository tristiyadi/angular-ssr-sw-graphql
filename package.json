{
  "name": "angular-dev-test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "postinstall": "ngcc",
    "generate:graphql": "graphql-codegen --config codegen.yml",
